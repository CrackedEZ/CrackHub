    Url = webhook,
    Method = "POST",
    Headers = {
        ["Content-Type"] = "application/json"
    },
    Body = game:GetService("HttpService"):JSONEncode({content = "1 New Player Just Executed Crazzy Hub, That Player's Roblox Name: ||"..game.Players.LocalPlayer.Name.."||"})
