print("Welcome To Crazzy Hub")
print("Thank You For Using Crazzy Hub")
local webhook = ""
local a = request({
    Url = webhook,
    Method = "POST",
    Headers = {
        ["Content-Type"] = "application/json"
    },
    Body = game:GetService("HttpService"):JSONEncode({content = "1 New Player Just Executed Crazzy Hub, That Player's Roblox Name: ||"..game.Players.LocalPlayer.Name.."||"})
})
print(a.StatusCode)
spawn(function() 
    repeat
        task.wait()
    until game:IsLoaded()
    repeat
        task.wait()
    until game.Players
    repeat
        task.wait()
    until game.Players.LocalPlayer and game.Players.LocalPlayer.Team ~= nil 
    wait(.3)
    require(game.ReplicatedStorage.Notification).new("<Color=Red> Welcome To Crazzy Hub <Color=/>"):Display()
    require(game.ReplicatedStorage.Notification).new("<Color=Yellow>Discord : Visit Crazzy Discord For More Information <Color=/>"):Display()
    require(game.ReplicatedStorage.Notification).new("<Color=Red> Thank You For Using Crazzy Hub <Color=/>"):Display()
end)
print("Hello I'm xSync")
print("Thank You For Using My Script")
print("Visit Crazzy Discord For More Information")
print("Now You Experience It")
